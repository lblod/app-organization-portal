PREFIX adms: <http://www.w3.org/ns/adms#>
PREFIX generiek: <https://data.vlaanderen.be/ns/generiek#>
PREFIX persoon: <https://data.vlaanderen.be/ns/persoon#>

CONSTRUCT {
  ?s ?p ?o .
} WHERE {
  ?s ?p ?o .
  FILTER EXISTS {
    ?s a ?type .
    VALUES ?type {
      adms:Identifier
      generiek:GestructureerdeIdentificator
      persoon:Geboorte
    }
  }
}
